pipeline {
agent  {
label 'DevServer'
}

parameters {
string defaultValue: 'Thangavelu', name: 'LASTNAME'
}

environment {
NAME= "Agathian"
}

tools {
    maven 'myMaven'
    }


  stages{
    stage ('Build'){
    steps {
      sh 'mvn clean package'
    echo "Hello $NAME ${params.LASTNAME}"
    }

	post{
	success{
	archiveArtifacts artifacts: '**/target/*.war'
			}
		}
   }
	stage('test')
			{
			parallel {
					stage ('test A'){
						steps {
						echo " This is test A"
						}
						
					stage ('test B'){
						steps
						echo " This is test B"
						}
						
				}
  			}
  }
  
  }

